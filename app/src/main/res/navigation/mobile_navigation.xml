<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_us">

    <fragment
        android:id="@+id/navigation_us"
        android:name="com.example.covid_19.ui.us.USFragment"
        android:label="@string/title_us"
        tools:layout="@layout/fragment_usa" />
    <fragment
        android:id="@+id/navigation_search"
        android:name="com.example.covid_19.ui.search.SearchFragment"
        android:label="@string/title_search"
        tools:layout="@layout/fragment_search" />
    <fragment
        android:id="@+id/navigation_world"
        android:name="com.example.covid_19.ui.world.WorldFragment"
        android:label="@string/title_world"
        tools:layout="@layout/fragment_world" />
    <fragment
        android:id="@+id/navigation_news"
        android:name="com.example.covid_19.ui.news.NewsFragment"
        android:label="@string/title_news"
        tools:layout="@layout/fragment_news" />
    <fragment
        android:id="@+id/navigation_user"
        android:name="com.example.covid_19.ui.user.UserFragment"
        android:label="@string/title_user"
        tools:layout="@layout/fragment_user" />

</navigation>
